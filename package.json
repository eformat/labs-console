{
  "name": "labs-console",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=4",
    "npm": ">=3.8"
  },
  "main": "server.js",
  "dependencies": {
    "async": "^1.5.0",
    "babel-polyfill": "^6.9.1",
    "basic-auth": "^1.1.0",
    "body-parser": "^1.15.2",
    "c3": "^0.4.14",
    "classnames": "^2.2.5",
    "cors": "^2.7.1",
    "crypto": "0.0.3",
    "d3": "^3.5.17",
    "express": "^4.13.4",
    "fastclick": "^1.0.6",
    "history": "^3.0.0",
    "immutability-helper": "^2.1.2",
    "jquery": "^3.1.1",
    "jquery-match-height": "^0.7.0",
    "method-override": "~2.0.2",
    "moment": "^2.15.1",
    "mongodb": "^2.1.16",
    "mongoose": "^4.5.7",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-validator": "^1.2.5",
    "morgan": "^1.7.0",
    "object-assign": "4.1.0",
    "patternfly": "^3.24.0",
    "react": "^15.2.1",
    "react-dnd": "^2.3.0",
    "react-dnd-mouse-backend": "^0.1.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "superagent": "^2.3.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "browser-sync": "^2.13.0",
    "chai": "^3.5.0",
    "connect-history-api-fallback": "^1.2.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "ejs": "^2.5.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "file-loader": "^0.9.0",
    "front-matter": "^2.1.0",
    "highlight.js": "^9.5.0",
    "jest": "^16.0.0",
    "json-loader": "^0.5.4",
    "markdown-it": "^7.0.0",
    "mocha": "^2.5.3",
    "path-to-regexp": "^1.5.3",
    "pixrem": "^3.0.1",
    "pleeease-filters": "^3.0.0",
    "postcss": "^5.1.1",
    "postcss-calc": "^5.3.0",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nesting": "^2.3.1",
    "postcss-selector-matches": "^2.0.1",
    "postcss-selector-not": "^2.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.0.3",
    "stylelint-config-standard": "^11.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "array-callback-return": [
        "off"
      ],
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "camelcase": [
        "off"
      ],
      "consistent-return": [
        "off"
      ],
      "comma-dangle": [
        "off"
      ],
      "eqeqeq": [
        "error",
        "always"
      ],
      "import/extensions": [
        "off"
      ],
      "import/no-dynamic-require": [
        "off"
      ],
      "import/no-extraneous-dependencies": [
        "off"
      ],
      "import/prefer-default-export": [
        "off"
      ],
      "jsx-a11y/href-no-hash": [
        "off"
      ],
      "jsx-a11y/no-static-element-interactions": [
        "off"
      ],
      "max-len": [
        "error",
        130
      ],
      "new-cap": [
        "off"
      ],
      "no-bitwise": [
        "off"
      ],
      "no-console": [
        "off"
      ],
      "no-mixed-operators": [
        "off"
      ],
      "no-new": [
        "off"
      ],
      "no-param-reassign": [
        "off"
      ],
      "no-plusplus": [
        "off"
      ],
      "no-prototype-builtins": [
        "off"
      ],
      "no-restricted-syntax": [
        "off"
      ],
      "no-underscore-dangle": [
        "off"
      ],
      "func-names": [
        "off"
      ],
      "react/no-unescaped-entities": [
        "off"
      ],
      "react/prefer-stateless-function": [
        "off"
      ],
      "react/jsx-closing-bracket-location": [
        "error",
        "after-props"
      ],
      "react/forbid-prop-types": [
        "off"
      ],
      "react/jsx-filename-extension": [
        "off"
      ],
      "react/jsx-first-prop-new-line": [
        "error",
        "never"
      ],
      "react/jsx-no-bind": "off",
      "react/jsx-indent": [
        "error",
        2
      ],
      "react/jsx-space-before-closing": [
        "error",
        "never"
      ],
      "react/prop-types": [
        "off"
      ]
    },
    "env": {
      "jquery": true
    },
    "globals": {
      "CustomEvent": true,
      "document": true,
      "Event": true,
      "HTMLElement": true,
      "location": true,
      "MutationObserver": true,
      "navigator": true,
      "requestAnimationFrame": true,
      "window": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single"
    }
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec.js)$",
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/test/frontend/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "eslint": "eslint components pages test main.js",
    "stylelint": "stylelint \"components/**/*.css\" \"pages/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "clean": "node run clean",
    "build": "node run build",
    "build:debug": "node run build --debug",
    "publish": "node run publish",
    "publish:debug": "node run publish --debug",
    "start": "node server.js",
    "postinstall": "node run build; [ \"$BUILD_ENV\" != production ] && exit 0; npm prune --production"
  }
}
